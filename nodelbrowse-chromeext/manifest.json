{
  "name": "Nodel browser",
  "description": "Browses the Nodel network.",
  "manifest_version": 2,
  "version": "0.6",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "permissions": [
    {
      "socket": [
        "udp-bind:*",
        "udp-send-to:*"
      ]
    }
  ],
  "icons": { "128": "img/icon-128.png", "16": "img/icon-16.png" }
}
